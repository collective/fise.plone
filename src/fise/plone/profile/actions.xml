<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
	xmlns:i18n="http://xml.zope.org/namespaces/i18n">
	<object name="document_actions" meta_type="CMF Action Category">
		<object name="FiseEnhancements" 
		        meta_type="CMF Action"
			    i18n:domain="fise.plone">
			<property name="title" i18n:translate="">
				FISE enhancements
			</property>
			<property name="description" i18n:translate="">
				Apache Stanbol FISE RAW enhancements
			</property>
			<property name="url_expr">
				string:${object/absolute_url}/@@fiseenhancements
			</property>
			<property name="icon_expr"></property>
			<property name="permissions">
				<element value="View" />
			</property>
			<property name="visible">True</property>
		</object>
	</object>
</object>
